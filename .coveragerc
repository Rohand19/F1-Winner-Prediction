[run]
source = src/f1predictor
branch = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
ignore_errors = True
omit =
    tests/*
    setup.py
    docs/*
    */site-packages/*
    */__init__.py

[html]
directory = coverage_html

[xml]
output = coverage.xml 